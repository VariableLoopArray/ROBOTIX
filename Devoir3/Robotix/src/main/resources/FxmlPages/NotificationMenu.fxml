        <Pane prefWidth="40"/>
        <VBox alignment="TOP_CENTER">
            <Pane prefHeight="40"/>
            <Label text="Notification Menu" style="-fx-font-size: 24px" alignment="CENTER"/>
            <Pane prefHeight="60"/>

        </VBox>
    </HBox>

</StackPane>

